{
  "extends": [
    "react-app",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "semi": ["error", "always", { "omitLastInOneLineBlock": false}],
    "quotes": [2, "single", { "avoidEscape": true }],
    "linebreak-style": "off"
  }
}